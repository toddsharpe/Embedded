BinName=Bootrom.elf
Toolchain=Stm32

[IncludeDirs]
>Inc
>External/cmsis_device_f7/Include
>External/CMSIS_5/CMSIS/Core/Include
>External/Graphics/Src
>External/printf

[CSources]
system_stm32f7xx.c
>Stm32/sysmem.c
>syscalls.c
>External/printf/printf.c

[CppSources]
Main.cpp
>Stm32/DmaStream.cpp
>Stm32/EthMac.cpp
>Stm32/Flash.cpp
>Stm32/Mdio.cpp
>Stm32/Nucleo-F746ZG.cpp
>Stm32/Pll.cpp
>Stm32/Rcc.cpp
>Stm32/SysCfg.cpp
>Stm32/SystemClock.cpp

[AsmSources]
startup_stm32f746zgtx.s

[Defines]
STM32F746xx
COLOR_RGB565R

[Flags]
-O3
-Wall
-fdata-sections
-ffunction-sections
-g -gdwarf-2

[CppFlags]
-std=c++17
-fno-exceptions
-fno-rtti

[LinkFlags]
-Wl,--defsym=FLASH_ORIGIN=0x8000000
-Wl,--defsym=FLASH_LENGTH=64k
-TStm32/STM32F746ZGTX_FLASH.ld

[PostBuild]
$ObjCopy: -O binary -S $OUTDIR/Bootrom.elf $OUTDIR/Bootrom.bin
$ObjCopy: -O ihex $OUTDIR/Bootrom.elf $OUTDIR/Bootrom.hex
