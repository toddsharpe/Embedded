BinName=Bootrom
Toolchain=Stm32

[IncludeDirs]
>Inc
>External/cmsis_device_f7/Include
>External/CMSIS_5/CMSIS/Core/Include
>External/Graphics/Src
>External/printf

[CSources]
>Stm32/sysmem.c
>syscalls.c
>External/printf/printf.c

[CppSources]
Main.cpp
>Stm32/system_stm32f7xx.cpp
>Stm32/HandleException.cpp
>Stm32/Pll.cpp
>Stm32/Rcc.cpp
>Stm32/SysCfg.cpp
>Stm32/SystemClock.cpp
>Stm32/Boards/Nucleo-F746ZG.cpp
>Sys/IsrVector.cpp
>Boards/Printer.cpp

[AsmSources]
>Stm32/startup_stm32f746zgtx.s

[Defines]
STM32F746xx

[Flags]
-O3
-Wall
-fdata-sections
-ffunction-sections
-g -gdwarf-2

[CppFlags]
-std=c++17
-fno-exceptions
-fno-rtti

[LinkFlags]
-Wl,--defsym=FLASH_ORIGIN=0x8000000
-Wl,--defsym=FLASH_LENGTH=64k
-TStm32/STM32F746ZGTX_FLASH.ld
-Wl,-Map=$BinFile.map,--cref

[PostBuild]
$ObjCopy: -O binary -S $BinFile.elf $BinFile.bin
$ObjCopy: -O ihex $BinFile.elf $BinFile.hex
$ObjDump: -S --disassemble $BinFile.elf > $BinFile.disasm
$ObjDump: -s -j .isr_vector $BinFile.elf > $BinFile.isr_vector
$Stat: $BinFile.bin
